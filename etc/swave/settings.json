{
  "client":
  {
    "name": "sing-box",
    "path_config": "/opt/etc/sing-box/configs",
    "lockFile": "/opt/tmp/singbox-swave.lock"
  },
  "app":
  {
    "name": "swave",
    "log": "/opt/var/log/swave/swava.log"
  },
  "network":
  {
    "table_id": "222",
    "table_mark_hex": "0x4629",
    "connection_policy": "swave",
    "chain_name": "swave",
    "port_redirect": "60008",
    "port_tproxy": "60009",
    "port_forwarding_list":
    [
      80,
      443,
      8080
    ],
    "port_forwarding_range":[
      "1024:1055",
      "1000:1099"
    ],
    "IPv6": true,
    "IPv6_exclusions":[
      "2a01:7e00::/29", //IP адреса ваших корпоративных серверов
      "2001:19f0::/32"
    ],
    "IPv4_exclusions":[
      "104.131.0.0/16", //IP адреса ваших корпоративных серверов
      "143.42.0.0/16",
      "159.203.0.0/16"
    ],
    "dns_filter": true
  }
}
