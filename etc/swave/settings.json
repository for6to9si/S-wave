{
  "client":
  {
    "name": "sing-box",
    "path_config": "/opt/etc/sing-box/configs"
  },
  "app":
  {
    "name": "swave",
    "version": "0.0.11",
    "log": "/opt/var/log/swave/swava.log"
  },
  "network":
  {
    "table_id": "222",
    "table_mark_hex": "0x4629",
    "connection_policy": "swave",
    "chain_name": "swave",
    "port_redirect": "60008",
    "port_tproxy": "60009",
    "modules":
    [
      "xt_TPROXY.ko",
      "xt_socket.ko",
      "xt_owner.ko"
    ],
    "module_path": "/lib/modules/4.9-ndm-5",
    "port_forwarding_list":
    [
      80,
      443,
      8080
    ],
    "port_forwarding_range":[
      "1024:1055",
      "1000:1099"
    ],
    "IPv6": false,
    "IPv6_exclusions":[
      "2a01:7e00::/29", //IP адреса ваших корпоративных серверов
      "2001:19f0::/32"
    ],
    "IPv4_exclusions":[
      "104.131.0.0/16", //IP адреса ваших корпоративных серверов
      "143.42.0.0/16",
      "159.203.0.0/16"
    ],
    "dns":{
      "dns_filter": true,
      "IPv4":[
        "10.0.0.0/8",
        "192.168.0.0/16"
      ],
      "IPv6":[
        "fd00::/8",
        "fe80::/10"
      ]
    }
  }
}
